<soap:Body>
	<Fetch xmlns="http://schemas.microsoft.com/crm/2007/WebServices">
		<fetchXml>
			<fetch version="1.0" output-format="xml-platform" mapping="logical"
				distinct="true" page="1" count="1">
				<entity name="account">
					<attribute name="name" />
					<attribute name="statuscode" />
					<attribute name="accountid" />
					<attribute name="websiteurl" />
					<attribute name="surf_isinstelling" />
					<attribute name="surf_ss_authentication_domain" />
					<order attribute="name" descending="false" />
					<link-entity name="lmng_licenseagreement" from="lmng_organisationid"
						to="accountid" alias="license">
						<attribute name="lmng_number" />
						<attribute name="lmng_validfrom" />
						<attribute name="lmng_validto" />
						<filter type="and">
							<condition attribute="lmng_validfrom" operator="on-or-before"
								value="%VALID_ON%" />
							<condition attribute="lmng_validto" operator="on-or-after"
								value="%VALID_ON%" />
							<condition attribute="statuscode" operator="eq" value="4" />
						</filter>
						<link-entity name="lmng_productvariation" from="lmng_productvariationid"
							to="lmng_productvariationid" alias="productvariation">
							<attribute name="lmng_name" />
							<attribute name="lmng_description" />
							<attribute name="lmng_deliverymethodid" />
							<filter type="and">
								<condition attribute="lmng_licensemodel" operator="eq"
									value="2" />
							</filter>
							<link-entity name="lmng_product" from="lmng_productid"
								to="lmng_productid" alias="product">
								<attribute name="lmng_name" />
								<attribute name="lmng_description" />
								<attribute name="lmng_descriptionlong" />
								<link-entity name="lmng_image" from="lmng_imageid"
									to="lmng_productimageid" alias="image" link-type="outer">
									<attribute name="lmng_url" />
								</link-entity>
								<link-entity name="account" from="accountid" to="lmng_accountid"
									alias="supplier">
									<attribute name="name" />
								</link-entity>
								<link-entity name="lmng_mediationagreement" from="lmng_mediationagreementid"
									to="lmng_mediationagreementid" alias="mediationagreement">
									<filter type="and">
										<condition attribute="lmng_agreementtype" operator="eq"
											value="1" />
									</filter>
								</link-entity>
							</link-entity>
						</link-entity>
					</link-entity>
					<link-entity name="contact" from="contactid" to="primarycontactid"
						visible="false" link-type="outer" alias="contact">
						<attribute name="fullname" />
						<attribute name="emailaddress1" />
						<attribute name="jobtitle" />
					</link-entity>
				</entity>
			</fetch>
		</fetchXml>
	</Fetch>
</soap:Body>
