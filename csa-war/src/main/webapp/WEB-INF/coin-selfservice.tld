<taglib xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-jsptaglibrary_2_1.xsd"
        version="2.1">

  <tlib-version>1.0</tlib-version>
  <short-name>coin-selfservice</short-name>
  <uri>http://www.surfnet.org/jsp/coin-selfservice/1.0</uri>
  <tag>
    <name>arpFilter</name>
    <tag-class>nl.surfnet.coin.selfservice.util.ARPFilter</tag-class>
    <body-content>scriptless</body-content>
    <attribute>
      <description>Name of the exported scoped variable to hold the result</description>
      <name>var</name>
      <required>true</required>
      <rtexprvalue>false</rtexprvalue>
      <type>java.lang.String</type>
    </attribute>
    <attribute>
      <description>Identifier of the IdP</description>
      <name>idpId</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <type>java.lang.String</type>
    </attribute>
    <attribute>
      <description>List of nl.surfnet.coin.selfservice.domain.ARP</description>
      <name>arpList</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
      <type>java.util.List</type>
    </attribute>
  </tag>
</taglib>
