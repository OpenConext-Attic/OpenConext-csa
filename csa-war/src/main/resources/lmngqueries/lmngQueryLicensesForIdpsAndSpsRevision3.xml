<fetch version="1.0" output-format="xml-platform" mapping="logical" distinct="true">
  <entity name="lmng_licenseagreement" alias="license">
    <attribute name="lmng_licenseagreementid"/>
    <attribute name="lmng_number"/>
    <attribute name="lmng_validfrom"/>
    <attribute name="lmng_validto"/>
    <attribute name="lmng_organisationid"/>
    <filter type="and">
      <condition attribute="lmng_validfrom" operator="on-or-before" value="%VALID_ON%" />
      <condition attribute="lmng_validto" operator="on-or-after" value="%VALID_ON%" />
      <condition attribute="statuscode" operator="eq" value="4"/>
      %INSTITUTION_CONDITION%
    </filter>
    <link-entity name="lmng_mediationagreement" from="lmng_mediationagreementid" to="lmng_mediationagreementid" alias="aa">
      <link-entity name="lmng_mediationagreement" from="lmng_source" to="lmng_mediationagreementid" alias="ab">
        <link-entity name="lmng_mediationagreement" from="lmng_source" to="lmng_mediationagreementid" alias="abe">
          <link-entity name="lmng_product" from="lmng_mediationagreementid" to="lmng_mediationagreementid" alias="product">

            <link-entity name="lmng_sdnarticle_lmng_product" from="lmng_productid" to="lmng_productid" visible="false" intersect="true">
              <link-entity name="lmng_sdnarticle" from="lmng_sdnarticleid" to="lmng_sdnarticleid" alias="ad">
                <filter>
                  <condition attribute="lmng_sdnarticleid" operator="in">
                    %ARTICLE_CONDITION_VALUES%
                  </condition>
                </filter>
              </link-entity>
            </link-entity>
          </link-entity>
        </link-entity>
      </link-entity>
    </link-entity>
  </entity>
</fetch>