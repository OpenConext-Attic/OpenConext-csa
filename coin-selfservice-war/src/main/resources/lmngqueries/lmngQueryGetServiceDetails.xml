<fetch version="1.0" output-format="xml-platform" mapping="logical"
	distinct="true">
	<entity name="lmng_productvariation" from="lmng_productvariationid"
		to="lmng_productvariationid" alias="productvariation">
		<attribute name="lmng_name" />
		<attribute name="lmng_productvariationid" />
		<attribute name="lmng_description" />
		<attribute name="lmng_deliverymethodid" />
		<filter type="and">
			<condition attribute="lmng_licensemodel" operator="eq"
				value="2" />
		</filter>
		<link-entity name="lmng_product" from="lmng_productid" to="lmng_productid"
			alias="product">
			<attribute name="lmng_name" />
			<attribute name="lmng_description" />
			<attribute name="lmng_descriptionlong" />
			<link-entity name="lmng_image" from="lmng_imageid" to="lmng_productimageid"
				alias="image" link-type="outer">
				<attribute name="lmng_url" />
			</link-entity>
			<link-entity name="account" from="accountid" to="lmng_accountid"
				alias="supplier">
				<attribute name="name" />
			</link-entity>
			<link-entity name="lmng_mediationagreement" from="lmng_mediationagreementid"
				to="lmng_mediationagreementid" alias="mediationagreement">
				<filter type="and">
					<condition attribute="lmng_agreementtype" operator="eq"
						value="1" />
				</filter>
			</link-entity>
			<link-entity name="lmng_sdnarticle_lmng_product" from="lmng_productid"
				to="lmng_productid" visible="false" intersect="true">
				<link-entity name="lmng_sdnarticle" from="lmng_sdnarticleid"
					to="lmng_sdnarticleid" alias="artikel">
					<filter type="and">
						<condition attribute="lmng_sdnarticleid" operator="in">
							<value>%SERVICE_ID%</value>
						</condition>
					</filter>
					<all-attributes />
				</link-entity>
			</link-entity>
		</link-entity>
	</entity>
</fetch>